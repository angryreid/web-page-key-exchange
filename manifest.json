{
  "manifest_version": 3,
  "name": "Domain Specific Script Injector",
  "version": "1.0",
  "description": "Injects a JavaScript file into a specific domain.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://web.shanbay.com/wordsweb/*"],
      "js": ["contentScript.js"]
    }
  ]
}
